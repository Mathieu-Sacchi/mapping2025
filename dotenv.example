# API Keys
# Single API key
GEMINI_API_KEY=your_api_key_here
# Multiple API keys (comma-separated for quota distribution)
GEMINI_API_KEYS=key1,key2,key3

# Model Configuration
MODEL=gemini-2.0-flash

# File Paths
EXISTING_FILE=existing_mapping.csv
NEW_FILE=CB.xlsx
OUTPUT_FILE=updated_mapping.csv
CHECKPOINT_FILE=checkpoint_processed.txt

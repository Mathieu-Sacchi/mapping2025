# Runtime Configuration
MAX_RETRIES=5
BATCH_SIZE=15
PAUSE_SECONDS=60

# Parallelization Configuration
MAX_WORKERS=4
QUOTA_PER_MINUTE=15
WAIT_AFTER_QUOTA=60 